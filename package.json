{
  "name": "youtubar",
  "version": "0.0.1",
  "description": "menubar app for Youtube",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src -d lib --copy-files",
    "watch": "rimraf lib && babel src -d lib --copy-files --watch",
    "dev": "electron .",
    "lint": "eslint {src,test} --fix",
    "unittest": "mocha --compilers js:babel-register --recursive test --timeout 10000",
    "test": "npm run lint && npm run unittest",
    "package": "electron-packager . youtubar --out build --overwrite --ignore=src --platform=mas --arch=x64 --icon youtubar.icns"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtoxin/youtubar"
  },
  "keywords": [],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "private": true,
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/youtubar/issues"
  },
  "homepage": "https://github.com/airtoxin/youtubar#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-preset-stage-3": "6.17.0",
    "babel-register": "6.18.0",
    "devtron": "1.4.0",
    "electron": "1.4.13",
    "electron-packager": "8.4.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "mocha": "3.2.0",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "babel-runtime": "6.20.0",
    "baobab": "2.3.4",
    "baobab-react": "2.1.2",
    "lodash": "4.17.2",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  }
}
